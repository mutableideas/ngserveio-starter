{
  "projectType": "application",
  "sourceRoot": "apps/ng-serve-io/playground-app/src",
  "prefix": "ng-serve-io",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ng-serve-io/playground-app",
        "index": "apps/ng-serve-io/playground-app/src/index.html",
        "main": "apps/ng-serve-io/playground-app/src/main.ts",
        "polyfills": "apps/ng-serve-io/playground-app/src/polyfills.ts",
        "tsConfig": "apps/ng-serve-io/playground-app/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/ng-serve-io/playground-app/src/favicon.ico",
          "apps/ng-serve-io/playground-app/src/assets"
        ],
        "styles": [
          "apps/ng-serve-io/playground-app/src/styles.scss",
          "apps/ng-serve-io/playground-app/src/playground-app.theme.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/ng-serve-io/playground-app/src/environments/environment.ts",
              "with": "apps/ng-serve-io/playground-app/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "ng-serve-io-playground-app:build:production"
        },
        "development": {
          "browserTarget": "ng-serve-io-playground-app:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "ng-serve-io-playground-app:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/ng-serve-io/playground-app/**/*.ts",
          "apps/ng-serve-io/playground-app/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/ng-serve-io/playground-app"],
      "options": {
        "jestConfig": "apps/ng-serve-io/playground-app/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["domain:ng-serve-io", "platform:ng", "type:ng-app"]
}
